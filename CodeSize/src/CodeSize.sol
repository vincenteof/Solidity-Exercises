pragma solidity ^0.8.13;

contract CodeSize {
    bytes public constant LARGE_DATA0 =
        hex"";
    bytes public constant LARGE_DATA1 =
        hex"";
    /**
     * The challenge is to create a contract whose runtime code (bytecode) size is greater than 1kb but less than 4kb
     */

    function getData0() public pure returns (bytes memory) {
        return LARGE_DATA0;
    }

    function getData1() public pure returns (bytes memory) {
        return LARGE_DATA0;
    }
}
